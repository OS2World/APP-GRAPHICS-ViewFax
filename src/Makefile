# ----------------------------------------------------------------------
# start of Imakefile

# Imakefile for viewfax

HELPDIR = ${USRLIBDIR}/X11
DEFINES = -DHELPFILE=\"$(HELPDIR)/viewfax.tif\"

SRCS = viewfax.c faxinput.c faxinit.c faxexpand.c
OBJS = viewfax.o faxinput.o faxinit.o faxexpand.o
SYS_LIBRARIES = $(XLIB)
DEPLIBS = $(DEPXLIB)

PROGRAM = viewfax.exe

all:: viewfax.exe

viewfax.exe: $(OBJS)
	$(RM) $@
	$(CCLINK) -o $@ $(LDOPTIONS) $(OBJS) $(LOCAL_LIBRARIES) $(LDLIBS)  $(EXTRA_LOAD_FLAGS)

install:: viewfax.exe
	$(MKDIRHIER) $(DESTDIR)$(BINDIR)
	$(INSTALL) $(INSTALLFLAGS) $(INSTPGMFLAGS)  viewfax.exe $(subst \,/,$(DESTDIR)$(BINDIR))/viewfax.exe

install.man:: viewfax.man
	$(MKDIRHIER) $(DESTDIR)$(MANDIR)
	imakesvc 12 viewfax $(DESTDIR)$(MANDIR) viewfax

depend::
	$(subst /,\,$(DEPEND)) $(DEPENDFLAGS) -- $(ALLDEFINES) $(DEPEND_DEFINES) -- $(SRCS)

clean::
	$(RM) $(PROGRAM)

install:: viewfax.tif
	$(INSTALL) $(INSTALLFLAGS) $(INSTDATFLAGS) viewfax.tif $(DESTDIR)$(HELPDIR)

# ----------------------------------------------------------------------
# common rules for all Makefiles - do not edit

.c.i:
	$(RM) $@
	 $(CC) -E $(CFLAGS) $(_NOOP_) $*.c > $@

emptyrule::

